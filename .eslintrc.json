{
  "env": {
    "es2021": true,
    "browser": true
  },
  "globals": {
    "global": false,
    "__VERSION__": false
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint:recommended", // https://eslint.org/docs/rules/
  "rules": {
    // 0 or "off" - turn the rule off
    // 1 or "warn" - turn the rule on as a warning (doesnâ€™t affect exit code)
    // 2 or "error" - turn the rule on as an error (exit code will be 1)

    // Possible Errors
    "no-console": 1,
    // "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-empty": 1,
    "no-empty-character-class": 1,
    "no-extra-parens": 0,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    // "no-inner-declarations": 0,
    // "no-inner-declarations": 1, -- delete?
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-obj-calls": 1,
    "no-prototype-builtins": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-template-curly-in-string": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 1,
    "no-unsafe-finally": 1,
    "use-isnan": 1,
    "valid-jsdoc": [1, {
      "requireReturn": false,
      // "requireReturnType": true,
      "requireParamDescription": true,
      "requireReturnDescription": true
    }],
    "valid-typeof": 1,

    // Best Practices
    "accessor-pairs": 1,
    "array-callback-return": 1,
    "complexity": 0,
    "consistent-return": 1,
    "curly": [ 1, "multi-or-nest" ],
    "default-case": 0,
    "dot-location": [ 1, "property" ],
    "dot-notation": 1,
    "eqeqeq": 1,
    "guard-for-in": 0,
    "no-alert": 1,
    "no-caller": 1,
    "no-case-declarations": 1,
    "no-div-regex": 0,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-empty-pattern": 1,
    "no-eq-null": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 0,
    "no-implicit-coercion": [ 1, {
      "boolean": false,
      "number": true,
      "string": true
    }],
    "no-implicit-globals": 1,
    "no-invalid-this": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-magic-numbers": 0,
    "no-multi-spaces": [ 1, {
      "exceptions": {
        "Property": true,
        "VariableDeclarator": true
      }
    }],
    "no-multi-str": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 0,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-self-assign": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unmodified-loop-condition": 1,
    "no-unused-expressions": 1,
    "no-unused-labels": 1,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-useless-escape": 0,
    "no-void": 1,
    "no-warning-comments": 1,
    "no-with": 2,
    "radix": [ 1, "as-needed" ],
    "wrap-iife": [ 1, "inside" ],
    "yoda": [ 1, "never", {
      "exceptRange": true
    }],

    // Strict Mode
    "strict": [ 1, "global" ],

    // Variables
    "init-declarations": 0,
    // "no-catch-shadow": 1,
    "no-delete-var": 1,
    "no-label-var": 1,
    "no-restricted-globals": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-undef": 1,
    "no-undef-init": 1,
    "no-undefined": 0,
    "no-unused-vars": 1,
    "no-use-before-define": [ 1, "nofunc" ],

    // Node.js and CommonJS
    "callback-return": 1,
    "global-require": 1,
    "handle-callback-err": 1,
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-path-concat": 1,

    // Stylistic Issues
    "array-bracket-newline": [ 1, "consistent"],
    "array-bracket-spacing": [ 1, "never" ],
    "array-element-newline": [ 1, "consistent"],
    "block-spacing": [ 1, "always" ],
    "brace-style": [ 1, "1tbs", {
      "allowSingleLine": false
    }],
    "camelcase": [ 1, {
      "properties": "always"
    }],
    "comma-dangle": [ 1, "always-multiline" ],
    "computed-property-spacing": [ 1, "never" ],
    "eol-last": [ 1, "never" ],
    "func-names": [ 1, "always" ],
    "func-style": [ 1, "declaration", {
      "allowArrowFunctions": true
    }],
    "indent": [ 1, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": {
        "var": 1,
        "let": 1,
        "const": 1
      },
      "MemberExpression": 1,
      "ArrayExpression": 1,
      "ObjectExpression": 1
    }],
    "key-spacing": [ 1, {
      "beforeColon": false,
      "afterColon": true,
      "mode": "minimum"
    }],
    "keyword-spacing": [ 1, {
      "before": true,
      "after": true
    }],
    "linebreak-style": [ 1, "unix" ],
    "lines-around-comment": [ 1, {
      "beforeBlockComment": true
    }],
    "max-depth": [ 1, 4 ], // 6
    "max-len": [ 1, {
      "code": 100,
      "tabWidth": 2,
      "ignoreComments": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreRegExpLiterals": true,
      "ignorePattern": "^import\\s.+\\sfrom\\s.+;$"
    }],
    "max-lines": 0,
    "max-nested-callbacks": [ 1, {
      "max": 4
    }],
    "max-params": [ 1, {
      "max": 5
    }],
    "max-statements": 0,
    "max-statements-per-line": [ 1, {
      "max": 2
    }],
    "new-cap": [ 1, {
      "newIsCap": true,
      "capIsNew": true,
      "properties": false
    }],
    "new-parens": 1,
    "newline-per-chained-call": [ 1, {
      "ignoreChainWithDepth": 4
    }],
    "no-array-constructor": 1,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-operators": [ 1, {
      "allowSamePrecedence": true
    }],
    "no-mixed-spaces-and-tabs":[ 1, "smart-tabs" ],
    "no-multi-assign": 1,
    "no-multiple-empty-lines": [ 1, {
      "max": 3,
      "maxEOF": 0,
      "maxBOF": 0
    }],
    "no-negated-condition": 0,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-plusplus": 0,
    "no-restricted-syntax": [ 1, "WithStatement" ],
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "no-whitespace-before-property": 1,
    "nonblock-statement-body-position": [ 1, "below" ],
    "object-curly-newline": 0,
    "object-curly-spacing": [ 1, "always" ],
    "object-property-newline": 1,
    "one-var": [ 1, "consecutive" ],
    "one-var-declaration-per-line": [ 1, "initializations" ],
    "operator-linebreak": [ 1, "after" ],
    "padded-blocks": 0,
    "quote-props": [ 1, "as-needed" ],
    "quotes": [ 1, "single", {
      "allowTemplateLiterals": true
    }],
    "require-jsdoc": [ 1, {
      "require": {
        "FunctionDeclaration": false,
        "ClassDeclaration": true,
        "MethodDefinition": true
      }
    }],
    "semi": [ 1, "always" ],
    "semi-spacing": [ 1, {
      "before": false,
      "after": true
    }],
    "sort-keys": [ 1, "asc", {
      "caseSensitive": false,
      "natural": true
    }],
    "sort-vars": 0,
    "space-before-blocks": [ 1, "always" ],
    "space-before-function-paren": [ 1, {
      "named": "never"
    }],
    "space-in-parens": [ 1, "always", {
      "exceptions": [ "{}", "[]" ]
    }],
    "space-infix-ops": 1,
    "space-unary-ops": [ 1, {
      "words": true,
      "nonwords": false
    }],
    "spaced-comment": [ 1, "always"/* , {
      "line": {
        "exceptions": [ "-", "+", "*" ]
      }
    } */],
    "wrap-regex": 1,

    // ECMAScript 6
    "arrow-body-style": 1,
    "arrow-parens": 1,
    "arrow-spacing": [ 1, {
      "before": true,
      "after": true
    }],
    "constructor-super": 1,
    "generator-star-spacing": [ 1, {
      "before": true,
      "after": false
    }],
    "no-class-assign": 1,
    "no-confusing-arrow": [ 1, {
      "allowParens": true
    }],
    "no-const-assign": 1,
    "no-dupe-class-members": 1,
    "no-duplicate-imports": 1,
    "no-new-symbol": 1,
    "no-restricted-imports": 0,
    "no-this-before-super": 1,
    "no-useless-computed-key": 1,
    "no-useless-constructor": 1,
    "no-useless-rename": 1,
    "no-var": 1,
    "object-shorthand": 1,
    "prefer-arrow-callback": 0,
    "prefer-const": 1,
    "prefer-destructuring": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-template": 1,
    "require-yield": 1,
    "rest-spread-spacing": 1,
    "sort-imports": 0,
    "template-curly-spacing": 1,
    "yield-star-spacing": [ 1, "before" ]
  }
}